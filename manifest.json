{
  "manifest_version": 3,
  "name": "Copy Markdown And Hatena Embed Link",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  },
  "permissions": ["activeTab", "clipboardWrite"]
}